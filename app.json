{
  "expo": {
    "name": "lolympia",
    "slug": "lolympia",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/lolympic_icon.png",
    "splash": {
      "image": "./assets/lolympic_icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#2b2d3d"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pedersapp.lolympia",
      "buildNumber": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/lolympic_icon.png",
        "backgroundColor": "#2b2d3d"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}